{
  "manifestVersion": 1,
  "id": "trigger-release-with-opcions",
  "version": "1.0.0",
  "name": "Trigger a Release definition with options",
  "description": "Trigger a Release definition with options",
  "publisher": "gregorypilar",
  "public": true,
  "icons": {
    "default": "static/images/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": ["Azure Pipelines, Trigger a Release, Release Task"],
  "categories": ["Azure Pipelines"],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "README.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/gregorypilar/ado-trigger-release"
  },
  "files": [
    {
      "path": "trigger-release"
    }
  ],
  "contributions": [
    {
      "id": "trigger-release",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "trigger-release"
      }
    }
